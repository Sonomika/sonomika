{
  "name": "p5vj",
  "version": "1.0.0",
  "description": "A VJ application built with Electron and p5.js",
  "main": "dist-electron/main.js",
  "scripts": {
    "start": "electron .",
    "build": "tsc && vite build",
    "build:watch": "tsc --watch",
    "dev": "vite",
    "dev:electron": "concurrently \"vite\" \"wait-on tcp:localhost:5173 tcp:localhost:5174 tcp:localhost:5175 && electron .\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "electron",
    "p5.js",
    "vj",
    "visual",
    "effects"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^20.10.5",
    "@types/p5": "^1.7.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-slider": "^1.3.6",
    "@types/three": "^0.159.0",
    "@types/webmidi": "^2.0.10",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.15.5",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/uuid": "^10.0.0",
    "chokidar": "^3.5.3",
    "p5": "^1.11.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-slider": "^2.0.6",
    "three": "^0.159.0",
    "uuid": "^11.1.0",
    "wavesurfer.js": "^7.10.1",
    "webmidi": "^3.1.8",
    "zustand": "^4.4.7"
  }
}
