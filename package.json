{
  "name": "sonomika",
  "version": "1.0.0",
  "description": "A VJ application built with Electron and p5.js",
  "main": "out/electron/main.js",
  "scripts": {
    "start": "electron .",
    "build": "tsc -p tsconfig.electron.json && tsc && vite build",
    "build:watch": "tsc --watch",
    "build:electron": "tsc -p tsconfig.electron.json",
    "dev": "vite",
    "dev:web": "vite --config vite.web.config.ts",
    "build:web": "vite build --config vite.web.config.ts",
    "vercel-build": "vite build --config vite.web.config.ts",
    "dev:electron": "npm run build:electron && concurrently \"vite\" \"wait-on tcp:localhost:5173 tcp:localhost:5174 tcp:localhost:5175 && electron .\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "electron",
    "p5.js",
    "vj",
    "visual",
    "effects"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
